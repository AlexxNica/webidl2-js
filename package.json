{
  "name": "webidl2-js",
  "version": "0.0.1",
  "description": "A parse for relaxed Web IDL 2. This parser originally followed the draft specification and has since been extended to afford common deviations from the spec.",
  "main": "src/main.js",
  "scripts": {
    "test": "karma start ./config/karma.all.conf.js",
    "test_unit": "karma start ./config/karma.unit.conf.js",
    "test_integration": "karma start ./config/karma.integration.conf.js",
    "test_dev": "karma start ./config/karma.dev.conf.js",
    "test_node": "JASMINE_CONFIG_PATH=./config/jasmine.json jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdittmer/webidl2-js.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": "Mark Dittmer",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mdittmer/webidl2-js/issues"
  },
  "homepage": "https://github.com/mdittmer/webidl2-js#readme",
  "eslintConfig": {
    "extends": "google"
  },
  "dependencies": {
    "foam2-experimental": "git://github.com/foam-framework/foam2-experimental.git"
  },
  "devDependencies": {
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^1.8.1",
    "webidl2": "git://github.com/w3c/webidl2.js.git"
  }
}
